        \node (2) at (1.5,-2) {$v_3$};
        \node (3) at (3,0) {$v_4$};
        \node (4) at (3,4) {$v_5$};
        \node (5) at (4.5,2) {$v_6$};
        \node (6) at (6,0) {$v_7$};
        \node (7) at (4.5,-2) {$v_8$};
