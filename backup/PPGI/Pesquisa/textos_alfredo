\label{fig:spannig_with_scha}
\end{figure}

% Note that, now the decoders are used to build a forest and the SCHA computes the MBT its. The adaptions are the Algorithms \ref{alg:bphi_decoder} and \ref{alg:frfshi_decoder}.

\begin{algorithm}[ht!]
\SetAlFnt{\small\sf}
\SetKwInOut{Input}{Input}\SetKwInOut{Output}{Output}
\Input{Connected undirected graph: $G = (V, E)$, \\ Source set: $V_0$, \\ Chromosomes vector: $Cr$, \\ Alpha: $\alpha$, \\ Beta: $\beta$, \\ Distances vector: $Dist$, \\Degree vector: $Degree$ }
\Output{Total step time to broadcast: $time$}
\DontPrintSemicolon
\SetKwProg{Fn}{}{}{}
\SetKwFunction{BPHI}{BPHI}
\SetKwFunction{BestCandidate}{BestCandidate}
\SetKwFunction{MBTForest}{MBTForest}
\SetKwFunction{MBTTree}{MBTTree}
\SetKwFunction{KwFn}{Fn}
\BlankLine
\Fn{\BestCandidate{$G, T, v, \alpha, \beta, Cr, Dist, Degree$}} {
    \If{$ (N(v) \setminus T) \neq \emptyset $}{
        \Return{$min_{u \in N(v) \setminus T}\{( Cr[u] + \frac{\alpha}{Dist[u]} + \frac{\beta}{Degree[u]} ,u)\}$}
    } 
    \Return{$\emptyset$}
}

\Fn{\BPHI{$G, V_0, Cr, \alpha, \beta, Dist, Degree$}} {
    $G_{b} \gets \emptyset$\;
    $time \gets 0$\;
    $Ranking \gets \emptyset$\;
    $Transmitters \gets V_0$\;
    \For{each $v \in V_0$}{
        $Ranking \gets Ranking \cup \{(Cr[v], v)\}$\;
    }
    \While{$Transmitters \neq V$} {
        $V_{time} \gets \emptyset$\;
        $Sort(Ranking)$ \gray{\tcp*{Ordering in ascending order of allele value}}
        \For{each $(r,v) \in Ranking$} {
            $(c,u) \gets BestCandidate(G, Transmitters \cup V_{time}, v, \alpha, \beta, Cr, Dist, Degree)$\;
            \If{$(c,u) \neq \emptyset$}{
                $V_{time} \gets V_{time} \cup \{u\}$\;
                $G_{b} \gets G_{b} \cup \{(v,u)\}$\;
            }
        }
        \For{each $v \in V_{time}$} {
            $Transmitters \gets Transmitters \cup \{v\}$\;
            $Ranking \gets Ranking \cup \{(Cr[v],v)\}$\;
        }
    }
    \Return{$MBT\_Forest(G_{b}, V_0)$}
}
\caption{Based on Priority With Heuristics and SCHA Decoder}\label{alg:bphi_decoder}
\end{algorithm}

\begin{algorithm}[ht!]
\SetAlFnt{\small\sf}
\SetKwInOut{Input}{Input}\SetKwInOut{Output}{Output}
\Input{Connected undirected graph: $G = (V, E)$, \\ Source set: $V_0$, \\ Chromosomes vector: $Cr$, \\ Alpha: $\alpha$, \\ Beta: $\beta$, \\ Distances vector: $Dist$, \\Degree vector: $Degree$ }
\Output{Total step time to broadcast: $time$}
\DontPrintSemicolon
\SetKwProg{Fn}{}{}{}
\SetKwFunction{FRFSHI}{FRFSHI}
\SetKwFunction{BestCandidate}{BestCandidate}
\SetKwFunction{MBTForest}{MBTForest}
\SetKwFunction{MBTTree}{MBTTree}
\SetKwFunction{KwFn}{Fn}
\BlankLine
\Fn{\BestCandidate{$G, T, v, \alpha, \beta, Cr, Dist, Degree$}} {
    \If{$ (N(v) \setminus T) \neq \emptyset $}{
        \Return{$min_{u \in N(v) \setminus T}\{( Cr[u] + \frac{\alpha}{Dist[u]} + \frac{\beta}{Degree[u]} ,u)\}$}
    } 
    \Return{$\emptyset$}
}
\Fn{\FRFSHI{$G, V_0, Cr, \alpha, \beta, Dist, Degree$}} {
    $G_{b} \gets \emptyset$\;
    $time \gets 0$\;
    $Ranking \gets \emptyset$\;
    $Transmitters \gets V_0$\;
    \For{each $v \in V_0$}{
        $Ranking \gets Ranking \cup \{(Cr[v], v)\}$\;
    }
    $Sort(Ranking)$ \gray{\tcp*{Ordering in ascending order of allele value}}
    \While{$Transmitters \neq V$} {
        $V_{time} \gets \emptyset$\;
        \For{each $(r,v) \in Ranking$} {
            $(c,u) \gets BestCandidate(G, Transmitters \cup V_{time}, v, \alpha, \beta, Cr, Dist, Degree)$\;
            \If{$(c,u) \neq \emptyset$}{
                $V_{time} \gets V_{time} \cup \{u\}$\;
                $G_{b} \gets G_{b} \cup \{(v,u)\}$\;
            }
        }
        \For{each $v \in V_{time}$} {
            $Transmitters \gets Transmitters \cup \{v\}$\;
            $Ranking \gets Ranking \cup \{(Cr[v],v)\}$\;
        }
    }
    \Return{$MBT\_Forest(G_{b}, V_0)$}
}
\caption{First Receive First Send With Heuristics and SCHA Decoder}\label{alg:frfshi_decoder}
\end{algorithm}


