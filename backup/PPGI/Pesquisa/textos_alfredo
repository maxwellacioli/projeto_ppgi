		\label{fig:graph_edge_example}
	\end{minipage}\hfill
	\begin{minipage}{0.45\linewidth}

		\caption{Chromosomes of input}
		\label{table:chromosomes_edge}
		\centering
        \begin{tabular}{|c|c|c|c|} \hline
            \textbf{Edges} & \textbf{Value(\textit{Cr})} & \textbf{Edges} & \textbf{Value(\textit{Cr})} \\  \hline
            $v_1 - v_2$ & 0.15 & 
            $v_3 - v_8$ & 0.40 \\  \hline
            $v_1 - v_3$ & 0.20 &
            $v_4 - v_6$ & 0.25 \\ \hline
            $v_1 - v_4$ & 0.30 & 
            $v_4 - v_7$ & 0.50 \\ \hline
            $v_2 - v_4$ & 0.55 & 
            $v_4 - v_8$ & 0.45 \\ \hline
            $v_2 - v_5$ & 0.60 & 
            $v_5 - v_6$ & 0.35 \\ \hline
            $v_2 - v_6$ & 0.65 & 
            $v_6 - v_7$ & 0.70 \\ \hline
            $v_3 - v_4$ & 0.75 & 
            $v_7 - v_8$ & 0.05 \\ \hline
        \end{tabular}
	\end{minipage}
\end{table}

\begin{algorithm}[ht!]
\SetAlFnt{\small\sf}
\SetKwInOut{Input}{Input}\SetKwInOut{Output}{Output}
\Input{Connected undirected graph: $G = (V, E)$, \\ Source set: $V_0$, \\ Chromosomes vector: $Cr$, \\ Alpha: $\alpha$, \\ Beta: $\beta$, \\ Distances vector: $Dist$, \\Degree vector: $Degree$ }
\Output{Total step time to broadcast: $time$}
\DontPrintSemicolon
\SetKwProg{Fn}{}{}{}
\SetKwFunction{MSF}{MSF}
\SetKwFunction{Root}{Root}
\SetKwFunction{Merge}{Merge}
\SetKwFunction{KwFn}{Fn}
\BlankLine
\Fn{\Root{$v, parent$}} {
    \If{parent[v] < 0}{
        \Return{$v$}\;
    }
    $parent[v] \gets Root(parent[v], parent)$\;
    \Return{$parent[v]$}\;
}
\Fn{\Merge{$u, v, parent$}} {
    $u \gets Root(u, parent)$\;
    $v \gets Root(v, parent)$\;
    $parent[v] \gets parent[u]$\;
}
\Fn{\MSF{$G, V_0, Cr$}} {
    $G_{b} \gets \emptyset$\;
    \For{each $v \in V(G)$}{
        $parent[v] \gets -1$\;
    }
    $r \gets \forall v_0 \in V_0$\;
    \For{each $v_0 \in V_0 \ \{r\}$}{
        $Merge(r, v_0)$\;
    }
    
    $Ranking \gets \emptyset$\;
    \For{each $e \in E(G)$}{
        $Ranking \gets Ranking \cup \{(Cr[e], e_v, e_u)\}$\;
    }
    
    \While{$|E(G_{b})| \neq |V(G)| - |V_0|$} {
        $e \gets ExtractMin(Ranking)$\;
        \If{$Root(e_v, parent) \neq Root(e_u, parent)$}{
            $G_{b} \gets G_{b} \cup \{(e_v,e_u)\}$\;
            $Merge(e_v, e_u)$\;
        }
    }
    \Return{$MBT\_Forest(G_{b}, V_0)$}
}
\caption{MST and SCHA Decoder}\label{alg:msf_decoder}
\end{algorithm}


